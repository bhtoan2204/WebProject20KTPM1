                <h3> Registration</h3>
                <div class="well">
                    {{#if err}}
                    {{#each err}}
                    <div class="alert {{check}}">
                        <button type="button" class="close" data-dismiss="alert">×</button>
                        {{message}}
                    </div>
                    {{/each}}
                    {{/if}}
                    <form class="form-horizontal" method="post">
                        <h4>Your personal information</h4>
                        <div class="control-group">
                            <label class="control-label" for="inputFname1">Name<sup>*</sup></label>
                            <div class="controls">
                                <input type="text" id="inputFname1" placeholder="Full name" name="name">
                            </div>
                        </div>
                        <div class="control-group">
                            <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
                            <label class="control-label" for="email">Email<sup>*</sup></label>
                            <div class="controls">
                                <input type="text" id="email" placeholder="Email" name="email"><span id="result"></span>
                            </div>
                        </div>
                        <div class="control-group">
                            <label class="control-label" for="inputPassword1">Password <sup>*</sup></label>
                            <div class="controls">
                                <input type="password" id="inputPassword1" placeholder="Password" name="pass1">
                            </div>
                        </div>
                        <div class="control-group">
                            <label class="control-label" for="inputPassword1">Rewrite assword <sup>*</sup></label>
                            <div class="controls">
                                <input type="password" id="inputPassword1" placeholder="Password" name="pass2">
                            </div>
                        </div>
                        <div class="control-group">
                            <label class="control-label">Date of Birth <sup>*</sup></label>
                            <div class="controls">
                                <input type="date" id="birthday" name="dob">
                            </div>
                        </div>

                        {{!-- <div class="alert alert-block alert-error fade in">
